{
  "name": "pocketbook",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "drizzle:migration-generate": "drizzle-kit generate:pg --schema=./src/db/schema.ts",
    "drizzle:migration-push": "node -r esbuild-register src/db/migrate.ts",
    "drizzle:migrate": "drizzle-kit generate:pg --schema=./src/db/schema.ts && node -r esbuild-register src/db/migrate.ts",
    "drizzle:seed": "node -r esbuild-register src/db/seed.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "csv": "^6.3.5",
    "drizzle-orm": "^0.29.2",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4.11.0",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "drizzle-kit": "^0.20.9",
    "eslint": "^8.53.0",
    "eslint-config-next": "14.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.2"
  }
}
